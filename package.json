{
  "name": "xln-v3-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "setUp": "yarn install && cd ./client && yarn install ",
    "dev": "concurrently \"yarn  server\" \"yarn fastapi:server\" \"yarn hardhat:node\" \"yarn hardhat:run\" \"yarn client\" ",
    "start:build": "tsc -w",
    "start:run": "nodemon build/index.js",
    "start": "concurrently npm:start:*",
    "client": "cd ./client && yarn dev",
    "server": "nodemon -x ts-node --esm index.ts",
    "hardhat:node": "npx hardhat node",
    "hardhat:run": "npx hardhat run --network localhost scripts/deploy.cjs ",
    "fastapi:server": "cd ./api && uvicorn api:app --reload",
    "ci": "concurrently \"npm backend\" \"npm frontend\" ",
    "backend": "rm -rf node_modules && npm install",
    "frontend": "cd ./client && rm -rf node_modules && yarn install",
    "production-server": "pm2 start index.js",
    "production-client": "cd ./client2 && pm2 start npm -- dev"
  },
